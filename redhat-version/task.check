# release 10.0

~regexp: \s+ "release" \s+ (\S+) \s

code: <<RAKU
!raku
if matched() {
  my $r = "{capture().head}";
  my @foo = $r.split(".");
  say "redhat version found: $r, major: {@foo.head}, minor: { @foo.tail || ''}";
  update_state(%( version => $r, major => @foo.head, minor => @foo.tail || ""  ));
} else {
  update_state(%( version => "", major => "", minor => "" ));
}
RAKU



